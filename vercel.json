{
  "rewrites": [
    {
      "source": "/ahorros/(.*)",
      "destination": "https://ahorros-personales.vercel.app/$1"
    },
    {
      "source": "/ahorros",
      "destination": "https://ahorros-personales.vercel.app"
    },
    {
      "source": "/catalogo/(.*)",
      "destination": "https://catalogo-servicios-web.vercel.app/$1"
    },
    {
      "source": "/catalogo",
      "destination": "https://catalogo-servicios-web.vercel.app"
    },
    {
      "source": "/((?!ahorros|catalogo).*)",
      "destination": "/$1"
    }
  ]
}
